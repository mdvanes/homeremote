    "express": "^4.15.2",
    "file-loader": "^0.10.1",
    "flexboxgrid": "^6.3.1",
    "html-webpack-plugin": "^1.7.0",
    "iconv": "^2.2.1",
    "id3-writer": "^1.2.2",
    "material-ui": "^0.20.0",
    "node-sass": "^4.5.0",
    "npm-watch": "^0.1.8",
