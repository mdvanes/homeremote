{
  "name": "homeremote",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "writeGitInfo": "ts-node --project ./tsconfig.node.json writeGitInfo.ts",
    "serve:all": "nx run-many --target=serve --projects=server,client --parallel",
    "start": "yarn rescript build -with-deps && yarn writeGitInfo dev && yarn serve:all",
    "assets:copy": "mv apps/server/src/assets apps/server/src/assets_bu && mkdir apps/server/src/assets && cp -r dist/apps/client/* apps/server/src/assets/",
    "assets:restore": "rm -rf apps/server/src/assets/ && mv apps/server/src/assets_bu apps/server/src/assets",
    "build": "nx run client:build --prod && yarn assets:copy && nx run server:build --prod",
    "test": "nx run-many --target=test --projects=server,client --parallel",
    "test:ci": "nx run-many --target=test --projects=server,client --parallel --coverage",
    "test:client": "env-cmd -f apps/client/.env.test nx test --watchAll",
    "test:client:ci": "env-cmd -f apps/client/.env.test nx test --coverage",
    "test:server": "nx run server:test --watchAll",
    "test:server:ci": "nx run server:test --coverage",
    "lint": "nx run-many --target=lint --projects=server,client --parallel",
    "postinstall": "yarn rescript build -with-deps",
    "typecheck": "tsc --noEmit --incremental false",
    "validate": "yarn lint && yarn test:ci",
    "prebuild": "yarn writeGitInfo && yarn validate"
  },
  "private": true,
  "repository": "https://github.com/mdvanes/homeremote",
  "author": "M.D. van Es",
  "engine": "node >= 16.0.0",
  "dependencies": {
    "@ctrl/transmission": "^2.2.2",
