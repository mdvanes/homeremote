{
  "name": "HomeRemote",
  "version": "1.0.0-alpha.1",
  "description": "Remote for home automation",
  "scripts": {
    "start": "node app.js",
    "startdebug": "node app.js --debugremote | bunyan",
    "lint": "esw webpack.config.* app.js server --color",
    "lint:watch": "npm run lint -- --watch",
    "build": "npm run lint && node node_modules/.bin/webpack --config webpack.config.js",
    "devserver": "concurrently --kill-others \"node node_modules/.bin/webpack-dev-server --config webpack.config.js --content-base ./public --inline --hot\" \"npm run startdebug\""
  },
