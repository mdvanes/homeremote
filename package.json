{
  "name": "HomeRemote",
  "version": "1.1.6-alpha.6",
  "description": "Remote for home automation",
  "scripts": {
    "devserver": "concurrently --kill-others \"node node_modules/.bin/webpack-dev-server --config webpack.config.js --content-base ./public --inline --hot\" \"npm run watch\"",
    "build": "npm run lint && NODE_ENV=production node node_modules/.bin/webpack --config webpack.config.js -p",
    "start": "node server/app.js",
    "startdebug": "npm run lint && node server/app.js --debugremote | bunyan",
    "lint": "esw webpack.config.* server --color",
    "lint:watch": "npm run lint -- --watch",
    "watch": "npm-watch"
  },
  "watch": {
    "startdebug": [
      "server/**/*.js",
      "test/server/**/*.js"
    ]
  },
  "private": true,
  "repository": "",
  "author": "M.D. van Es",
  "license": "Closed",
  "engine": "node >= 4.2.1",
