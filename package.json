  "scripts": {
    "start:dev": "concurrently --kill-others \"BROWSER=none react-scripts start\" \"npm run watch\"",
    "start:devserver": "node server/app.js --debugremote | bunyan",
    "start": "node server/app.js | bunyan",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "npm-watch",
    "lint": "eslint src/**/*.{ts,tsx}"
  },
  "watch": {
    "start:devserver": [
      "server/**/*.js",
      "test/server/**/*.js"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
